{
    "name": "quvel/tenant",
    "description": "Multi-tenant support for the Quvel framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Quvel",
            "email": "support@quvel.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/database": "^12.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "phpunit/phpunit": "^12.4",
        "laravel/pint": "^1.25",
        "larastan/larastan": "^3.7",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Quvel\\Tenant\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Quvel\\Tenant\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint",
        "format-check": "vendor/bin/pint --test",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=512M",
        "refactor": "vendor/bin/rector process",
        "refactor-dry": "vendor/bin/rector process --dry-run"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quvel\\Tenant\\TenantServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}