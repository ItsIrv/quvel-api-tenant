{
    "name": "quvel/tenant",
    "description": "Multi-tenant support for the Quvel framework",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Quvel",
            "email": "support@quvel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Quvel\\Tenant\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Quvel\\Tenant\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Quvel\\Tenant\\TenantServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}